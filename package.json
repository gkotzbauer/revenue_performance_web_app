{
  "name": "revenue-performance-web-app",
  "version": "1.0.0",
  "private": true,
  "description": "Revenue Performance Analytics Web App",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && python app.py",
    "dev:frontend": "cd frontend && npm start",
    "build": "cd frontend && npm run build",
    "start": "cd backend && python app.py",
    "install:all": "npm install && cd frontend && npm install"
  },
  "dependencies": {
    "concurrently": "^8.2.2"
  },
  "devDependencies": {},
  "keywords": [
    "revenue",
    "performance",
    "analytics",
    "healthcare",
    "webapp"
  ],
  "author": "Greg Kotzbauer",
  "license": "MIT"
}